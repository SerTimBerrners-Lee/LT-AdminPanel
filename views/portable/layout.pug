    include ../script/alert.js


//- TOATS для всплытия при новых сообщениях
                    div(role="alert" aria-live="assertive" aria-atomic="true" class="toast" data-autohide="true" data-delay="20000")
                        div(class="toast-header")
                            img(src="..." class="rounded mr-2" alt="...")
                            strong(class="mr-auto") Bootstrap 1
                            small 11 mins ago
                            button(type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close")
                                span(aria-hidden="true") &times;
                        div(class="toast-body")    Hello, world! This is a toast message.